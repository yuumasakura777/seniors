


<h2>質問一覧</h2>
<%= link_to '新しく質問をする', new_question_path, class: 'btn btn-primary' %>
  <div class="co-md-12">
    <table class="table table-striped">
      <thead class="thead-light">
        <tr>
          <th>質問番号</th>
          <th>タイトル</th>
        </tr>
      </thead>
      <tbody>
        <% @questions.each do |question| %>
          <tr>
            <td><%= question.id %></td>
            <td><%= link_to question.title, question %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
